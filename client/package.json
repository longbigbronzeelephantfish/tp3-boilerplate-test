{
  "name": "client",
  "version": "1.0.0",
  "main": "index.js",
  "license": "UNLICENSED",
  "dependencies": {
    "axios": "^0.16.2",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-airbnb": "^2.4.0",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.1",
    "jsdoc": "^3.5.5",
    "jsdoc-babel": "^0.3.0",
    "mobx": "^3.3.1",
    "mobx-react": "^4.3.3",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^16.0.0",
    "semantic-ui-css": "^2.2.12",
    "semantic-ui-react": "^0.74.2",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.0.0-beta.3",
    "url-loader": "^0.6.2",
    "webpack": "^3.7.1",
    "webpack-dev-server": "^2.9.1",
    "webpack-merge": "^4.1.0"
  },
  "jest": {
    "modulePaths": [
      "<rootDir>/src"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/dist",
      "<rootDir>/tests.setup.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/tests.setup.js"
  },
  "scripts": {
    "dev": "webpack-dev-server --host 0.0.0.0 --watch-poll --config webpack.dev.js",
    "lint": "eslint --ext jsx ./src",
    "lint-fix": "eslint --ext jsx ./src --fix",
    "test": "jest",
    "doc": "jsdoc -c jsdoc.conf.json -r -d ./docs ./src",
    "publish": "webpack -p --config webpack.prod.js",
    "docker-test": "sudo docker build -t client-test -f ./Dockerfile.Testing . && sudo docker run -t client-test",
    "docker-publish": "sudo docker build -t client -f ./Dockerfile.Production . && sudo docker run --name client -p 80:80 -t client",
    "docker-stop": "sudo docker stop client"
  }
}
